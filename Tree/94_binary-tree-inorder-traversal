# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def inorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        def mid_order_traversal(root, result):
            if root == None:
                return
            mid_order_traversal(root.left, result)
            result.append(root.val)
            mid_order_traversal(root.right, result)
        
        result = []
        mid_order_traversal(root, result)

        return result
            
